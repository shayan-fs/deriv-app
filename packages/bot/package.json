{
  "version": "1.0.0",
  "name": "deriv-bot",
  "description": "Visual automation for deriv.app",
  "main": "dist/bot.js",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/.jest"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "tools.js"
    ]
  },
  "scripts": {
    "start": "eslint ./src && rimraf dist &&  webpack-dev-server --mode=development --open 'Google Chrome' --hot --inline --port 80",
    "serve": "webpack --mode=development --progress --watch --open 'Google Chrome' --hot --inline --port 80",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --mode=production --optimize-minimize --progress",
    "build:travis": "echo \"Error: no build:travis specified\"",
    "test": "eslint ./src && jest -w 1 --coverage",
    "test:eslint": "eslint ./src",
    "test:stylelint": "stylelint \"./src/**/*.s?(a|c)ss\"",
    "test:mocha": "jest -w 1 --coverage",
    "deploy": "gh-pages --dist '.' --src '{index.html,manifest.json,dist/**,CNAME}'",
    "deploy:clean": "echo \"Error: no deploy:staging specified\"",
    "deploy:folder": "gh-pages --dist '.' --src '{index.html,manifest.json,dist/**}' --dest",
    "deploy:staging": "d() { test -z $1 && echo 'Please specify branch.' && exit 1; (git show-branch $1) || exit 1;  git stash; git checkout $1; npm i; rm -rf branch/$1; mkdir -p branch/$1 ; gulp build-min; cp -r www/ branch/$1; gulp release-branch --branch $1;}; d",
    "deploy:production": "d() { git stash; git checkout master; npm i;gulp build-min;gulp release-master;}; d"
  },
  "author": "Binary.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.5",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@binary-com/binary-indicators": "^1.6.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "binary-live-api": "^29.0.0",
